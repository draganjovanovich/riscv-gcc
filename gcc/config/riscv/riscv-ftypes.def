/* Invoke DEF_RISCV_FTYPE (NARGS, LIST) for each prototype used by
   RISCV built-in functions, where:

      NARGS is the number of arguments.
      LIST contains the return-type code followed by the codes for each
        argument type.

   Invoke _RVV_INT_ITERATOR and _RVV_MASK_ITERATOR with DEF_RISCV_FTYPE to
   create bunch of prototype used by RISC-V vector built-in functions.  */

DEF_RISCV_FTYPE (0, (USI))
DEF_RISCV_FTYPE (1, (VOID, USI))

DEF_RISCV_FTYPE (1, (SI, SI))
DEF_RISCV_FTYPE (1, (DI, DI))

#define DEF_UNARY_VECTOR_MASKING_FTYPE(MLEN, N)	\
  DEF_RISCV_FTYPE (1, (VB##MLEN, VB##MLEN))	\
  DEF_RISCV_FTYPE (1, (SI, VB##MLEN))		\
  DEF_RISCV_FTYPE (1, (DI, VB##MLEN))

_RVV_MASK_ITERATOR(DEF_UNARY_VECTOR_MASKING_FTYPE)

#define DEF_BINARY_VECTOR_MASKING_FTYPE(MLEN, N)	\
  DEF_RISCV_FTYPE (2, (VB##MLEN, VB##MLEN, VB##MLEN))	\
  DEF_RISCV_FTYPE (2, (SI, VB##MLEN, VB##MLEN))		\
  DEF_RISCV_FTYPE (2, (DI, VB##MLEN, VB##MLEN))

_RVV_MASK_ITERATOR(DEF_BINARY_VECTOR_MASKING_FTYPE)

#define DEF_BINARY_VECTOR_FTYPE(SEW, LMUL, MLEN, MODE, SUBMODE)		\
  DEF_RISCV_FTYPE (2, (VB##MLEN, VI##SEW##M##LMUL, VI##SEW##M##LMUL))	\
  DEF_RISCV_FTYPE (2, (VB##MLEN, VUI##SEW##M##LMUL, VUI##SEW##M##LMUL))	\
  DEF_RISCV_FTYPE (2, (VB##MLEN, VI##SEW##M##LMUL, SUBMODE))		\
  DEF_RISCV_FTYPE (2, (VI##SEW##M##LMUL, VI##SEW##M##LMUL, VI##SEW##M##LMUL)) \
  DEF_RISCV_FTYPE (2, (VI##SEW##M##LMUL, VI##SEW##M##LMUL, SUBMODE))	\
  DEF_RISCV_FTYPE (2, (VUI##SEW##M##LMUL, VUI##SEW##M##LMUL, VUI##SEW##M##LMUL))

_RVV_INT_ITERATOR(DEF_BINARY_VECTOR_FTYPE)

/* Must not be expanded for LMUL==1 as that will duplicate function types
   already defined above.  */
#define DEF_REDUC_VECTOR_FTYPE(SEW, LMUL, MLEN, MODE, SUBMODE)		\
  DEF_RISCV_FTYPE (2, (VI##SEW##M1, VI##SEW##M1, VI##SEW##M##LMUL))	\
  DEF_RISCV_FTYPE (2, (VUI##SEW##M1, VUI##SEW##M1, VUI##SEW##M##LMUL))

_RVV_INT_ITERATOR_NOTM1(DEF_REDUC_VECTOR_FTYPE)

#define DEF_WIDENING_BINARY_VECTOR_FTYPE(SEW, LMUL, MLEN, VMODE, SMODE,	\
					 WSEW, WLMUL, WVMODE, WSMODE)	\
  DEF_RISCV_FTYPE (2, (VI##WSEW##M##WLMUL,				\
		       VI##SEW##M##LMUL, VI##SEW##M##LMUL)) 		\
  DEF_RISCV_FTYPE (2, (VI##WSEW##M##WLMUL, VI##SEW##M##LMUL, SMODE))	\
  DEF_RISCV_FTYPE (2, (VI##WSEW##M##WLMUL,				\
		       VI##WSEW##M##WLMUL, VI##SEW##M##LMUL))		\
  DEF_RISCV_FTYPE (2, (VI##WSEW##M##WLMUL,				\
		       VI##WSEW##M##WLMUL, SMODE))			\
  DEF_RISCV_FTYPE (2, (VUI##WSEW##M##WLMUL,				\
		       VUI##SEW##M##LMUL, VUI##SEW##M##LMUL)) 		\
  DEF_RISCV_FTYPE (2, (VUI##WSEW##M##WLMUL, VUI##SEW##M##LMUL, SMODE))	\
  DEF_RISCV_FTYPE (2, (VUI##WSEW##M##WLMUL,				\
		       VUI##WSEW##M##WLMUL, VUI##SEW##M##LMUL))		\
  DEF_RISCV_FTYPE (2, (VUI##WSEW##M##WLMUL,				\
		       VUI##WSEW##M##WLMUL, SMODE))

_RVV_WINT_ITERATOR(DEF_WIDENING_BINARY_VECTOR_FTYPE)

DEF_RISCV_FTYPE (2, (VF32M8, VF16M4, VF16M4))
DEF_RISCV_FTYPE (2, (VF32M8, VF16M4, C_HF))

DEF_RISCV_FTYPE (3, (VF32M8, VF16M4, VF16M4, VF32M8))
DEF_RISCV_FTYPE (3, (VF32M8, VF16M4, C_HF, VF32M8))

#define DEF_MASKED_UNARY_VECTOR_MASKING_FTYPE(MLEN, N) \
  DEF_RISCV_FTYPE (3, (VB##MLEN, VB##MLEN, VB##MLEN, VB##MLEN))

_RVV_MASK_ITERATOR(DEF_MASKED_UNARY_VECTOR_MASKING_FTYPE)

#define DEF_MASKED_BINARY_VECTOR_MASKING_FTYPE(MLEN, N) \
  DEF_RISCV_FTYPE (4, (VB##MLEN, VB##MLEN, VB##MLEN, VB##MLEN, VB##MLEN))

_RVV_MASK_ITERATOR(DEF_MASKED_BINARY_VECTOR_MASKING_FTYPE)

#define DEF_MASKED_BINARY_VECTOR_FTYPE(SEW, LMUL, MLEN, MODE, SUBMODE)	\
  DEF_RISCV_FTYPE (4, (VB##MLEN, VB##MLEN, VB##MLEN,			\
		       VI##SEW##M##LMUL, VI##SEW##M##LMUL))		\
  DEF_RISCV_FTYPE (4, (VB##MLEN, VB##MLEN, VB##MLEN,			\
		       VUI##SEW##M##LMUL, VUI##SEW##M##LMUL))		\
  DEF_RISCV_FTYPE (4, (VB##MLEN, VB##MLEN, VB##MLEN,			\
		       VI##SEW##M##LMUL, SUBMODE))			\
  DEF_RISCV_FTYPE (4, (VI##SEW##M##LMUL, VB##MLEN, VI##SEW##M##LMUL,	\
		       VI##SEW##M##LMUL, VI##SEW##M##LMUL))		\
  DEF_RISCV_FTYPE (4, (VI##SEW##M##LMUL, VB##MLEN, VI##SEW##M##LMUL,	\
		       VI##SEW##M##LMUL, SUBMODE))			\
  DEF_RISCV_FTYPE (4, (VUI##SEW##M##LMUL, VB##MLEN, VUI##SEW##M##LMUL,	\
		       VUI##SEW##M##LMUL, VUI##SEW##M##LMUL))

_RVV_INT_ITERATOR(DEF_MASKED_BINARY_VECTOR_FTYPE)

/* Must not be expanded for LMUL==1 as that will duplicate function types
   already defined above.  */
#define DEF_MASKED_REDUC_VECTOR_FTYPE(SEW, LMUL, MLEN, MODE, SUBMODE)	\
  DEF_RISCV_FTYPE (4, (VI##SEW##M1, VB##MLEN, VI##SEW##M1,		\
		       VI##SEW##M1, VI##SEW##M##LMUL))			\
  DEF_RISCV_FTYPE (4, (VUI##SEW##M1, VB##MLEN, VUI##SEW##M1,		\
		       VUI##SEW##M1, VUI##SEW##M##LMUL))

_RVV_INT_ITERATOR_NOTM1(DEF_MASKED_REDUC_VECTOR_FTYPE)

